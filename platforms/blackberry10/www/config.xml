    <access origin="http://54.88.187.36:3000/auth/linkedin/*" subdomains="true" />
    <access origin="http://54.172.235.48:3000" subdomains="true" />
    <access origin="http://54.209.43.90:3000" subdomains="true" />
    <access origin="http://54.187.184.106:3000" subdomains="true" />
